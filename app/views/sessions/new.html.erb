<%= form_for @user, url:"/sessions", method: :post do |f| %>
  <%= f.label :name %>
  <%= collection_select(:user, :name, User.all, :name, :name, prompt: "Please select your username from this dropdown") %><br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  <%= f.submit "Sign In" %>
<% end %>
